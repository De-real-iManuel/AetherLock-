{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_WS_URL": "@vite_ws_url",
    "VITE_SOLANA_RPC_URL": "@vite_solana_rpc_url",
    "VITE_ZETACHAIN_RPC_URL": "@vite_zetachain_rpc_url",
    "VITE_SOLANA_PROGRAM_ID": "@vite_solana_program_id",
    "VITE_ZETACHAIN_GATEWAY": "@vite_zetachain_gateway",
    "VITE_ZKME_APP_ID": "@vite_zkme_app_id",
    "VITE_ENABLE_VOICE": "@vite_enable_voice",
    "VITE_ENABLE_ANALYTICS": "@vite_enable_analytics"
  },
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false,
    "autoJobCancelation": true
  }
}
